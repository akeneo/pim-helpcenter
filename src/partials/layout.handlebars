<body data-spy="scroll" data-target="#navbar">

    <div id="mainContent">
        {{> @partial-block }}
    </div>
    {{> main-footer }}
    
</body>
<script>
    $(function(){
        var navbar = $('.navbar');
        $(window).scroll(function(){
            if($(window).scrollTop() <= 40){
                navbar.removeClass('scroll');
            } else {
              navbar.addClass('scroll');
            }
        });
    });
</script>
</html>
